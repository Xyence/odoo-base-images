{
  "regexManagers": [
    {
      "datasourceTemplate": "docker",
      "fileMatch": ["^Dockerfile$"],
      "matchStrings": [
        "FROM\\s+(?<depName>.*?):(?<currentValue>)"
      ]
    }
  ],
  "packageRules": [
    {
      "matchDatasources": ["docker"],
      "pinDigests": true
    }
  ],
  "extends": [
    "config:base"
  ],
  "semanticCommits": "enabled",
  "semanticCommitType": "build"
}
